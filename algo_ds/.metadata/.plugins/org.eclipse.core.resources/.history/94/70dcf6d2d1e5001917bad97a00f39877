#include<iostream>
#include<vector>
#include<iterator>
#include<iomanip>
#include<algorithm>
using namespace std;

#define MAX 10
#define INF 9999
int parent[MAX];
int g_rank[MAX];
class edges
{
private:
	int src;
	int dest;
	int weight;
public:
	edges(int u=0, int v=0, int w=0)
{
		this->src=u;
		this->dest=0;
		this->weight=0;
}
	friend ostream& operator<<(ostream& out, edges& other)
	{
		out<<other.src<<"->" <<other.dest<< " : " <<other.weight;
		return out;
	}
};
class graph
{
private:
	vector<edge> edges;
	int vertc_cnt;
	int edge_cnt;
	int mat[MAX][MAX];


};


